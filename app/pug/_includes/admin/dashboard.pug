					.site-content--cards.site-content--cards-editable.form-group
						h1(role="heading") My Account
						.site-content--cards-row
							h2 General Information
							div
								a.btn.btn-secondary.btn-edit-general-information(href="#", role="link", data-js="edit-general-information") Edit
								a.btn.btn-secondary.btn-update-general-information.hidden(href="#", role="link", data-js="update-general-information") Update
								a.btn.btn-light.btn-cancel-general-information.hidden(href="#", role="link", data-js="cancel-general-information") Cancel
								a.btn.btn-secondary(href="#", role="link") Change Password
						.site-content--uneditable-wrap
							p
								| Prasanna Kugananthan
								br
								| prasanna@baustein.ca
						.site-content--editable-wrap.hidden
							.row
								.col-sm-6
									.form-group--md
										input#userFirstName.form-group--md-input(type='text', required='required')
										span.form-group--md-highlight
										span.form-group--md-bar
										label.form-group--md-label(for='userFirstName') First Name
								.col-sm-6
									.form-group--md
